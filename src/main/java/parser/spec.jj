TOKEN : {
      <DIGIT: ["0"-"9"]>
    | <SYMBOL: ["_"]>
    | <MINUS: ("-"){1}>
    | <SINGLE_QUOTE: "'">
    | <DOUBLE_QUOTE: "\"">
    | <ASCII: (~[])>
